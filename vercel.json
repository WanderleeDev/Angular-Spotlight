{
  "version": 2,
  "routes": [
    {
      "src": "/es/(.*)",
      "dest": "/es/$1"
    },
    {
      "src": "/en/(.*)",
      "dest": "/en/$1"
    },
    {
      "src": "^/$",
      "has": [
        {
          "type": "header",
          "key": "accept-language",
          "value": "(?:^|,)\\s*es(?:-[^,]*)?(?:,|$)"
        }
      ],
      "headers": {
        "Location": "/es/"
      },
      "status": 308
    },
    {
      "src": "^/$",
      "headers": {
        "Location": "/en/"
      },
      "status": 308
    }
  ]
}
